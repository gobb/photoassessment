{
    "name": "kachkaev/assets-version-bundle",
    "type": "symfony-bundle",
    "description": "Allows autoincrementing assets version in parameters.yml.",
    "keywords": ["assets", "assetic", "version", "autoincrement", "config", "console"],
    "homepage": "http://github.com/kachkaev/KachkaevAssetsVersionBundle",
    "license": "MIT",

    "authors": [
        {
            "name": "Alexander Kachkaev",
            "email": "alexander@kachkaev.ru"
        },
		{
            "name": "Contributors",
            "homepage": "https://github.com/kachkaev/KachkaevAssetsVersionBundle/contributors"
        }
    ],

    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.0,<2.3-dev"
    },

    "require-dev": {
    },

    "suggest": {
        "symfony/assetic-bundle": "Integrates Assetic into Symfony2"
    },

    "autoload": {
        "psr-0": {
            "Kachkaev\\AssetsVersionBundle": ""
        }
    },

    "target-dir": "Kachkaev/AssetsVersionBundle"
}